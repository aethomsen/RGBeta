  RowBox[{"yu", ",", 
   RowBox[{"{", 
    RowBox[{"H", ",", "q", ",", "u"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"GroupInvariant", "\[Rule]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"del", "[", 
        RowBox[{
         RowBox[{"SU3c", "@", "fund"}], ",", "#2", ",", "#3"}], "]"}], 
       RowBox[{"eps", "[", 
        RowBox[{
         RowBox[{"SU2L", "@", "fund"}], ",", "#2", ",", "#1"}], "]"}]}], 
      "&"}], ")"}]}], ",", "\[IndentingNewLine]", 
   RowBox[{"CouplingIndices", "\[Rule]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"gen", "[", "#2", "]"}], ",", 
        RowBox[{"gen", "[", "#3", "]"}]}], "}"}], "&"}], ")"}]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Chirality", "\[Rule]", "Right"}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"AddYukawa", "[", 
  RowBox[{"yd", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Bar", "@", "H"}], ",", "q", ",", "d"}], "}"}], ",", 
